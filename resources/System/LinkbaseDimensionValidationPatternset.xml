            <Action method="CreateErrorMemo" uri="http://xbrl.org/2005/xbrldte/errors#HasHypercubeSourceError" />
        </Failure>
    </Pattern>
    <Pattern id="HasHypercubeSourceError_e"
             name="xbrldte:HasHypercubeSourceError_e"
             method="DtsXPath"
